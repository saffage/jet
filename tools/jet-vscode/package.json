{
  "name": "jet-vscode",
  "displayName": "Jet",
  "description": "Jet language support for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "jet",
        "aliases": ["Jet", "jet"],
        "extensions": [".jet"],
        "configuration": "./jet-cfg.json"
      }
    ],
    "grammars": [
      {
        "language": "jet",
        "scopeName": "source.jet",
        "path": "./syntaxes/jet.json"
      }
    ],
    "snippets": [
      {
        "language": "jet",
        "path": "./snippets/jet.json"
      }
    ]
  }
}
